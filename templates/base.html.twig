            <script src="{{ asset('js/main.js') }}"></script>
            <!--  Notifications Plugin    -->
            <script src="{{ asset('js/bootstrap-notify.js') }}"></script>
        {% endblock %}

        {% for message in app.flashes('success') %}
            <script>
                $.notify({
                            icon: 'add_alert',
                            message: '{{message}}'
                            },
                        {
                            type: 'success'
                        });
            </script>
        {% endfor %}
        {% for message in app.flashes('danger') %}
            <script>
                $.notify({
                            icon: 'add_alert',
                            message: '{{message}}'
                            },
                        {
                            type: 'danger'
                        });
            </script>
        {% endfor %}

